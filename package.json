  "scripts": {
    "build": "grunt offline",
    "build-online": "grunt online",
    "build-cdn": "grunt cdn",
    "deploy": "npm run build-cdn && gh-pages -d dist -b cdn",
    "package": "tizen package -t wgt -- dist -o .",
    "install-app": "tizen install -t 10.0.0.95 --name Binge.wgt",
    "init": "tizen run -t 10.0.0.95 -p bpeU1aRY4w.Binge",
    "start-tv-online": "npm run build-online && npm run package && npm run install-app --tv=10.0.0.95 && npm run init --tv=10.0.0.95",
    "start-tv": "npm run build && npm run package && npm run install-app --tv=10.0.0.95 && npm run init --tv=10.0.0.95",
    "start": "npm run start-tv",
    "start-online": "npm run start-tv-online",
    "run": "npm run start-tv --tv=7XCI6YW6TV5L4",
    "run2": "npm run start-tv --tv=10.0.0.95"
